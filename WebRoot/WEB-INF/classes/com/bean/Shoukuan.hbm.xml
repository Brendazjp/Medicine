<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bean.Shoukuan" table="t_shoukuan" >
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="shoukuanid"/>
		<property name="shitime" />
		<property name="shijine"/>
		<property name="xinyupf"/>
		<property name="shoukuanren"/>
		<many-to-one name="xiaoshou"/>
	</class>
</hibernate-mapping>