<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bean.Tuikuan" table="t_tuikuan" >
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="tkuanid"/>
		<property name="tkuantime" />
		<property name="tkuanjine"/>
		<property name="tkuanren"/>
		<many-to-one name="shoukuan"/>
	</class>
</hibernate-mapping>